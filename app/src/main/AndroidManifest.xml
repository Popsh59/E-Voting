<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_voting">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name=".ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CandidateInfo"></activity>
        <activity android:name=".ViewStudents" />
        <activity android:name=".StudentInfo" />
        <activity android:name=".SetRoles" />
        <activity android:name=".Admin" />
        <activity android:name=".UploadVideo" />
        <activity android:name=".HomeCandidate" />
        <activity android:name=".SelectCandidates" />
        <activity android:name=".ViewManifesto" />
        <activity android:name=".UpdateProfile" />
        <activity android:name=".ViewVotes" />
        <activity android:name=".ViewCandidates" />
        <activity android:name=".Home" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>